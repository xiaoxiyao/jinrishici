{
  "name": "jinrishici-ts",
  "version": "1.0.2",
  "description": "获取今日诗词，更优雅的esm和ts类型支持",
  "keywords": [
    "jinrishici",
    "typescript",
    "今日诗词",
    "诗词api"
  ],
  "author": "xiyao.luoxiao@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@8.6.12",
  "type": "module",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/bundle.cjs.js",
      "import": "./dist/bundle.esm.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "tsup src/*.ts --watch src",
    "publishPkg": "npm publish",
    "test_dev": "node ./examples/index.js"
  },
  "homepage": "https://github.com/xiaoxiyao/jinrishici",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/xiaoxiyao/jinrishici",
  "bugs": {
    "url": "https://github.com/xiaoxiyao/jinrishici/issues"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "rollup": "^2.79.0",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  }
}